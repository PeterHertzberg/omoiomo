          "testimonial": [
            {
              "text": "<p>Hello! I'm Peter and OMOiOMO is my vessel to try to find my way back to that magical land of stories where I would dream away my days as a child...</p>",
              "media": {
                "image": {
                  "src": "@PROJECT_PATH@/assets/images/dsc-0915-20141204110910750-995x995-2000x2000.jpg",
